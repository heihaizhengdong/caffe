# The train/test net protocol buffer definition
net: "examples/fcn/fcn-32s-pascal.prototxt"
test_iter: 100
test_interval: 1000
base_lr: 0.001
lr_policy: "step"
gamma: 0.9
stepsize: 5000
display: 10
max_iter: 50000
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "examples/fcn/fcn"
# solver mode: CPU or GPU
solver_mode: GPU

