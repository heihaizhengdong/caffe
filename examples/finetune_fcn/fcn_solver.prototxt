# The train/test net protocol buffer definition
#net: "test.prototxt"
net: "fcn_finetune_train.prototxt"
test_iter: 1
test_interval: 1000
base_lr: 0.0000001
lr_policy: "step"
gamma: 0.5
stepsize: 1000
display: 1
max_iter: 100000
momentum: 0.99
weight_decay: 0
snapshot: 1000
snapshot_prefix: "fcn"
# solver mode: CPU or GPU
solver_mode: GPU

